{
  "name": "gatsby-starter",
  "private": true,
  "description": "An opinionated Gatsby starter with styled-components",
  "version": "1.0.0",
  "author": "Chase Ohlson <chase@chaseohlson.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.8.2",
    "gatsby-image": "^2.0.41",
    "gatsby-plugin-google-analytics": "^2.0.19",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.0.36",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-webpack-size": "^0.0.3",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-sharp": "^2.1.21",
    "husky": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "chrome-launcher": "^0.10.5",
    "core-js": "^3.1.3",
    "directory-named-webpack-plugin": "^4.0.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-eslint": "^2.0.3",
    "lighthouse": "^4.0.0",
    "prettier": "^1.17.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "yarn build && yarn serve",
    "format": "prettier --config ./.prettierrc --write src/**/**/*.js && prettier --config ./.prettierrc --write src/**/*.js",
    "build": "gatsby build",
    "test": "ava **/*.test.js --verbose",
    "clean": "rm -rf ./node_modules && rm -rf ./.cache && yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brohlson/gatsby-starter"
  }
}